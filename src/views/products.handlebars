<nav aria-label="Paginacion">
    <ul class="pagination">
        {{#if data.hasPrevPage}}<li class="page-item"><a class="page-link" href="/products?page={{data.prevPage}}">Anterior</a></li>{{/if}}
        <li class="page-item"><a class="page-link" href="/products?page=1">Pg 1</a></li>
        <li class="page-item"><a class="page-link" href="#">PÃ¡gina {{data.page}} de {{data.totalPages}}</a></li>
        {{#if data.hasNextPage}}<li class="page-item"><a class="page-link" href="/products?page={{data.nextPage}}">Siguiente</a></li>{{/if}}
    </ul>
</nav> 

{{#each data.docs}}
    <article class="container">
        <div class="card">
            <div class="imgBx">
                <img src="{{this.thumbnail}}" height="310" width="200"/>
            </div>
            <div class="contentBx" id="compra">
                <h2>{{this.title}}</h2>
                <p>${{this.price}}</p>
                <button class="botons" type="button" id="buy" href="#">Comprar</button>
            </div>
        </div>
    </article> 
{{/each}}


